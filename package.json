{
  "name": "synthetix-subgraph",
  "license": "MIT",
  "author": "Synthetix",
  "scripts": {
    "codegen:snx": "node index prepare-abis && graph codegen subgraphs/synthetix.yaml",
    "codegen:rates": "node index prepare-abis && graph codegen subgraphs/synthetix-rates.yaml",
    "codegen:exchanges": "node index prepare-abis && graph codegen subgraphs/synthetix-exchanges.yaml",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/",
    "deploy:snx": "npm run deploy -- --access-token $THEGRAPH_SNX_ACCESS_TOKEN synthetixio-team/synthetix subgraphs/synthetix.yaml",
    "deploy:rates": "npm run deploy -- --access-token $THEGRAPH_SNX_ACCESS_TOKEN synthetixio-team/synthetix-rates subgraphs/synthetix-rates.yaml",
    "deploy:exchanges": "npm run deploy -- --access-token $THEGRAPH_SNX_ACCESS_TOKEN synthetixio-team/synthetix-exchanges subgraphs/synthetix-exchanges.yaml",
    "create-local": "graph create --node http://localhost:8020/ synthetixio-team/synthetix",
    "remove-local": "graph remove --node http://localhost:8020/ synthetixio-team/synthetix",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 synthetixio-team/synthetix"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.15.1",
    "@graphprotocol/graph-ts": "0.15.1",
    "chalk": "2.4.2",
    "commander": "2.20.0",
    "ethers": "4.0.27",
    "synthetix": "2.11.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-prettier": "3.1.0",
    "prettier": "1.17.1",
    "typescript": "3.4.5"
  }
}
